</head>

<body style="background-color: rgb(231, 231, 231);">
    <div class="color mb-2">
        <nav class="navbar navbar-expand-lg align-middle">
            <div class="container-fluid color">
                <div class="navbar-brand color">
                    <img src="/image/flipcart-logo.jpg" alt="Flipcart" width="85" height="30">
                </div>
                <div>
                    <span class="navbar-toggler-icon color"></span>
                </div>
            </div>
        </nav>

    </div>

    <!-- part 2 -->
    <div class="navigation-bar container-fluid">
        <div class="navigation-item">
            <img src="/image/g-1.webp" alt="#">
            <span style="color: black; font-size: 13px;"><b>Grocery</b></span>
        </div>

        <div class="navigation-item">
            <img src="/image/g-2.webp" alt="#">
            <span style="color: black; font-size: 13px;"><b>Mobiles</b></span>
        </div>

        <div class="navigation-item">
            <img src="/image/g-3.webp" alt="#">
            <span style="color: black; font-size: 13px;"><b>Fashion</b></span>
        </div>

        <div class="navigation-item">
            <img src="/image/g-4.webp" alt="#">
            <span style="color: black; font-size: 13px;"><b>Electronics</b></span>
        </div>

        <div class="navigation-item">
            <img src="/image/g-5.webp" alt="#">
            <span style="color: black; font-size: 13px;"><b>Furniture</b></span>
        </div>

        <div class="navigation-item">
            <img src="/image/g-6.webp" alt="#">
            <span style="color: black; font-size: 13px;"><b>Appliances</b></span>
        </div>

        <div class="navigation-item">
            <img src="/image/g-7.webp" alt="#">
            <span style="color: black; font-size: 13px;"><b>Travel</b></span>
        </div>

        <div class="navigation-item">
            <img src="/image/g-8.webp" alt="#">
            <span style="color: black; font-size: 13px;"><b>Beauty</b></span>
        </div>
    </div>
    <!-- part 3 -->
    <div class="mt-2">

        <div id="carouselExampleAutoplaying" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="/image/Frame15.png" class="d-block w-100" alt="...">
                </div>
                <!-- <div class="carousel-item">
                    <img src="/image/image_2024_01_12T09_23_14_388Z.png" class="d-block w-100" alt="...">
                </div>
                <div class="carousel-item">
                    <img src="/image/image_2024_01_12T09_23_25_187Z.png" class="d-block w-100" alt="...">
                </div>
                <div class="carousel-item">
                    <img src="/image/image_2024_01_12T09_23_42_738Z.png" class="d-block w-100" alt="...">
                </div> -->
            </div>
            <!-- <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleAutoplaying"
                data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleAutoplaying"
                data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button> -->
        </div>
    </div>

    <div class="mt-5">
        <% products.forEach(product => { %>
        <div class="responsive">
            <div class="gallery">
                <a href="/add_cart/<%= product.id %>">
                    <% if (product.images && product.images.length > 0) { %>
                    <img class="product-image" src="<%= product.images[0].image %>" alt="#">
                    <% } %>
                </a>
            </div>
            <div class="gallery-1">
                <p style="font-size: 14px;"><b><%= product.brand %></b></p>
                <p style="font-size: 12px;"><%= product.name %> <span><img src="/image/fa_62673a.png" alt="#"
                            class="assured"></span>
                </p>
                <p><span style="color: #388e3c; font-size: 14px;"><b>71% off </b></span> <span
                        style="text-decoration: line-through; font-size: 14px;">
                        <b>₹<%= product.mrp %></b></span> <b style="font-size: 14px;">₹<%= product.price %></b> </p>
            </div>
        </div>
        <% }) %> 
    </div>
    <!-- ================================== -->


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
